{
  "RabbitMQ": {
    "Host": "localhost",
    "Port": 5672,
    "VirtualHost": "/ecommerce",
    "Username": "ecommerce_rabbit",
    "Password": "rabbit_password",
    "PrefetchCount": 32
  },
  "OpenTelemetry": {
    "Enabled": false,
    "ServiceName": "EcommerceAPI.API",
    "OtlpEndpoint": ""
  },
  "Redis": {
    "ConnectionString": null
  },
  "Cors": {
    "AllowedOrigins": [
      "http://ecommerce.berkansozer.com",
      "https://ecommerce.berkansozer.com"
    ]
  },
  "SignalR": {
    "RedisBackplaneEnabled": false,
    "ChannelPrefix": "ecommerce"
  },
  "Cache": {
    "ProductListTTLMinutes": 5,
    "DefaultTTLMinutes": 60
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "AllowedHosts": "localhost;127.0.0.1;ecommerce.berkansozer.com"
}
